<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.knowledgeaggregation">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.DOWNLOAD_WITHOUT_NOTIFICATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".recommend.RecommendActivity"></activity>
        <activity android:name=".htmlTest.HtmlTestActivity" />
        <activity
            android:name=".drawerlayouttest.DrawerTestActivity"
            android:label="@string/title_activity_drawer_test"
            android:theme="@style/NoActionBar" />
        <activity android:name=".resultlist.ArticleActivity" />
        <activity android:name=".JumpActivity" />
        <activity android:name=".register.InteresActivity" />
        <activity
            android:name=".userMessage.MessageChange2Activity"
            android:label="@string/title_activity_message_change2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".resultlist.ResultListActivity" />
        <activity android:name=".register.RegisterActivity" />
        <activity android:name=".search.SearchActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>